import React from 'react';
import ReactDOM from 'react-dom';

const name ='Nauman';
const currDate=new Date().toLocaleDateString();
const currTime=new Date().toLocaleTimeString();
ReactDOM.render(
  
  <>
  <h1> Hello! My name is {name}</h1>
<p>Date={currDate}</p>
<p>Time={currTime}</p>
  
  </>

  ,document.getElementById("root")
  
);
